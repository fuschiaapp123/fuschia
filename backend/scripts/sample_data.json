          "auth_type": "basic"
        },
        {
          "name": "update_incident",
          "method": "PUT",
          "url": "/api/now/table/incident/{id}",
          "auth_type": "basic"
        }
      ],
      "credentials": {
        "username": "fuschia_integration",
        "password": "encrypted_password"
      }
    },
    {
      "system": "Salesforce",
      "type": "rest_api",
      "endpoints": [
        {
          "name": "create_lead",
          "method": "POST",
          "url": "/services/data/v58.0/sobjects/Lead/",
          "auth_type": "oauth2"
        },
        {
          "name": "update_opportunity",
          "method": "PATCH",
          "url": "/services/data/v58.0/sobjects/Opportunity/{id}",
          "auth_type": "oauth2"
